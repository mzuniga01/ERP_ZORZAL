@model IEnumerable<ERP_GMEDINA.Models.tbMunicipio>

@using ERP_GMEDINA.Models;
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="ibox-title">
        <h5>Listado de Municipios</h5>
        <div class="ibox-tools">

        </div>
    </div>
    <div class="row">
        <table class="table table-striped table-responsive table-bordered table-hover" style="width:100%" id="DataTable">
            <thead>
                <tr>
                    <th class="col-lg-4">
                        @Html.DisplayNameFor(model => model.mun_Codigo)
                    </th>
                    <th class="col-lg-4">
                        @Html.DisplayNameFor(model => model.mun_Nombre)
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.mun_Codigo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.mun_Nombre)
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>






