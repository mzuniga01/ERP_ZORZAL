@model ERP_GMEDINA.Models.tbTipoEntrada

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/css/Auditoria.css" rel="stylesheet" />
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Tipo Entrada</h2>
    </div>
    <div class="col-sm-8">
        <div class="title-action">
            @Html.ActionLink("Regresar", "Index")
        </div>
    </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Detalles Tipo Entrada</h5>
                </div>
                <div class="ibox-content">
                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.tent_Id)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.tent_Id)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.tent_Descripcion)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.tent_Descripcion)
                        </dd>
                    </dl>

                    @Html.ActionLink("Editar", "Edit", new { id = Model.tent_Id }, new { @class = "btn btn-primary" })

                </div>
            </div>
            <div class="ibox-content">
                <table class="table table-striped">
                    <tr>
                        <th>
                            Acción
                        </th>
                        <th>
                            Usuario
                        </th>
                        <th>
                            Fecha
                        </th>
                    </tr>
                    <tr>
                        <td>
                            Creado
                        </td>
                        <td>
                            @ViewBag.UsuarioCrea
                        </td>
                        <td>
                            @Html.DisplayFor(model => model.tent_FechaCrea)
                        </td>
                    </tr>
                    <tr>
                        <td>
                            Modificado
                        </td>
                        <td>
                            @ViewBag.UsuarioModifica
                        </td>
                        <td>
                            @Html.DisplayFor(model => model.tent_FechaModifica)
                        </td>
                    </tr>
                </table>
            </div>
        </div>
     </div>
</div>





<script src="~/Scripts/jquery-3.1.1.min.js"></script>
<script src="~/Scripts/app/BusquedaGenerica.js"></script>








