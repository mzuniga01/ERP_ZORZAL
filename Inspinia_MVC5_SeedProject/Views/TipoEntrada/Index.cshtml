@model IEnumerable<ERP_GMEDINA.Models.tbTipoEntrada>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Tipo Entrada</h2>
    </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Listado Tipo Entrada</h5>
                    <div class="ibox-tools">
                        @Html.ActionLink("Registrar Tipo Entrada", "Create", null, new { @class = "btn btn-primary" })
                    </div>
                </div>
                <div class="ibox-content">

                    <table class="table table-striped table-responsive table-bordered table-hover" style="width:100%" id="Datatable">
                        <thead>

                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.tent_Id)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.tent_Descripcion)
                                </th>
                                <th>
                                    Acciones
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.tent_Id)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.tent_Descripcion)
                                    </td>
                                    <td>
                                        @Html.ActionLink("Detalles", "Details", new { id = item.tent_Id }, new { @class = "btn btn-primary btn-xs" })
                                        @Html.ActionLink("Editar", "Edit", new { id = item.tent_Id }, new { @class = "btn btn-white btn-xs " })
                                    </td>
                                </tr>
                            }

                        </tbody>
                    </table>
                </div>

            </div>
        </div>
    </div>
</div>

<script src="~/Scripts/jquery-3.1.1.min.js"></script>
<script src="~/Scripts/app/DataTable.js"></script>

