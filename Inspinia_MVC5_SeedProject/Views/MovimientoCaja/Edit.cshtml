@model ERP_GMEDINA.Models.tbMovimientoCaja

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    .validation-error {
        color: red;
        font-size: small;
    }
</style>

<link href="~/Content/css/Auditoria.css" rel="stylesheet" />

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Arqueo de Caja</h2>

    </div>
    <div class="col-sm-8">
        <div class="title-action">
            @Html.ActionLink("Regresar", "Index")
        </div>
    </div>
</div>





<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Arqueo de Caja</h5>
                </div>
                <div class="ibox-content">

                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()

                        <div class="form-horizontal">

                            @Html.ValidationSummary(true)
                            @Html.HiddenFor(model => model.mocja_Id)

                            @Html.HiddenFor(model => model.mocja_UsuarioCrea)
                            @Html.HiddenFor(model => model.mocja_FechaCrea)
                            @Html.HiddenFor(model => model.mocja_UsuarioModifica)
                            @Html.HiddenFor(model => model.mocja_FechaModifica)
                            @Html.HiddenFor(model => model.tbUsuario.usu_Nombres)
                            @Html.HiddenFor(model => model.tbUsuario.usu_Apellidos)
                            @Html.HiddenFor(model => model.tbUsuario1.usu_Nombres)
                            @Html.HiddenFor(model => model.tbUsuario1.usu_Apellidos)


                            <div class="form-group">
                                @Html.LabelFor(model => model.cja_Id, "Caja", new { @class = "control-label col-md-2" })
                                <div class="col-md-3">
                                    @Html.EditorFor(model => model.tbCaja.cja_Descripcion, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                    <span class="validation-error">@Html.ValidationMessageFor(model => model.cja_Id)</span>
                                </div>

                                <div class="form-group">
                                    @Html.LabelFor(model => model.mocja_UsuarioApertura, new { @class = "control-label col-md-2" })
                                    <div class="col-md-3">
                                        @Html.EditorFor(model => model.tbUsuario.usu_Nombres, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                        <span class="validation-error">@Html.ValidationMessageFor(model => model.mocja_UsuarioApertura)</span>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group">
                                @Html.LabelFor(model => model.mocja_UsuarioAceptacion, new { @class = "control-label col-md-2" })
                                <div class="col-md-3">
                                    @Html.EditorFor(model => model.tbUsuario.usu_Nombres, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                    <span class="validation-error">@Html.ValidationMessageFor(model => model.mocja_UsuarioAceptacion)</span>
                                </div>
                                <div class="form-group">
                                    @Html.LabelFor(model => model.mocja_FechaApertura, new { @class = "control-label col-md-2" })
                                    <div class="col-md-3">
                                        @Html.EditorFor(model => model.mocja_FechaApertura, "{0:yyyy/MM/dd}", new { htmlAttributes = new { @class = "form-control", @id = "fechaapertura", @readonly = "readonly" } })
                                        <span style="color:red" class="validation-error">@Html.ValidationMessageFor(model => model.mocja_FechaApertura)</span>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group">
                                @Html.LabelFor(model => model.mocja_UsuarioArquea, new { @class = "control-label col-md-2" })
                                <div class="col-md-3">
                                    @Html.EditorFor(model => model.tbUsuario.usu_Nombres, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                    <span class="validation-error">@Html.ValidationMessageFor(model => model.mocja_UsuarioArquea)</span>
                                </div>
                                <div class="form-group">
                                    @Html.LabelFor(model => model.mocja_FechaArqueo, new { @class = "control-label col-md-2" })
                                    <div class="col-md-3">
                                        @Html.EditorFor(model => model.mocja_FechaArqueo, "{0:yyyy/MM/dd}", new { htmlAttributes = new { @class = "form-control", @id = "fechaarqueo", @readonly = "readonly" } })
                                        <span class="validation-error">@Html.ValidationMessageFor(model => model.mocja_FechaArqueo)</span>
                                    </div>
                                </div>
                            </div>



                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="tabs-container">
                                        <ul class="nav nav-tabs">
                                            <li class="active"><a data-toggle="tab" href="#tab-1">Control Efectivo</a></li>
                                            <li class=""><a data-toggle="tab" href="#tab-2">Control de Pagos</a></li>
                                        </ul>
                                        <div class="tab-content">
                                            <div id="tab-1" class="tab-pane active">
                                                <div class="panel-body">
                                                    <div class="form-group">
                                                        <label class="control-label col-md-2">Efectivo Inicial</label>
                                                        <div class="col-md-3">
                                                            <input class="form-control" id="EfectivoInicial" readonly value="@ViewBag.EfectivoInicial" type="text" />
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="control-label col-md-2">Efectivo Recibido</label>
                                                            <div class="col-md-3">
                                                                <input type="text" value="" class="form-control" id="EfectivoRecibido" readonly />
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="form-group">
                                                        <label class="control-label col-md-2">Efectivo Pagos</label>
                                                        <div class="col-md-3">
                                                            <input type="text" class="form-control" id="EfectivoPagos" value="@ViewBag.PagoEfectivo" readonly />
                                                        </div>
                                                        <div class="form-group ">
                                                            <label class="control-label col-md-2">Efectivo Entregado</label>
                                                            <div class="col-md-3">
                                                                <input type="text" class="form-control" id="EfectivoEntregado" value="@ViewBag.EfectivoEntregado" readonly />
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <label class="control-label col-md-2">Total</label>
                                                        <div class="col-md-3">
                                                            <input type="text" class="form-control" id="TotalEfectivo" readonly />
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div id="tab-2" class="tab-pane">
                                                <div class="panel-body">
                                                    <div class="form-group">
                                                        <label class="control-label col-md-2">Pagos Efectivo</label>
                                                        <div class="col-md-3">
                                                            <input type="text" class="form-control" value="@ViewBag.PagoEfectivo" readonly />
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="control-label col-md-2">Pagos TC o TD</label>
                                                            <div class="col-md-3">
                                                                <input type="text" class="form-control" value="@ViewBag.PagoTC" readonly />
                                                            </div>
                                                        </div>
                                                    </div>


                                                    <div class="form-group">
                                                        <label class="control-label col-md-2">Pagos Cheque</label>
                                                        <div class="col-md-3">
                                                            <input type="text" class="form-control" value="@ViewBag.PagoCheque" readonly />
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="control-label col-md-2">Pagos Nota de Credito</label>
                                                            <div class="col-md-3">
                                                                <input type="text" class="form-control" value="@ViewBag.PagoNotaCredito" readonly />
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <label class="control-label col-md-2">Pagos Depósito</label>
                                                        <div class="col-md-3">
                                                            <input type="text" class="form-control" value="@ViewBag.PagoDeposito" readonly />
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="control-label col-md-2">Pagos Transferencia ACH</label>
                                                            <div class="col-md-3">
                                                                <input type="text" class="form-control" value="@ViewBag.PagoTranferencia" readonly />
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <label class="control-label col-md-2">Pagos Cupón</label>
                                                        <div class="col-md-3">
                                                            <input type="text" class="form-control" value="@ViewBag.PagoCuponDesc" readonly />
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="control-label col-md-2">Total Pagos</label>
                                                            <div class="col-md-3">
                                                                <input type="text" value="@ViewBag.TotalPagos" class="form-control" readonly />
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>


                            <div class="form-group m-t-xl">
                                <div class=" col-md-12">
                                    <label class="control-label">Detalle Denomonaciòn Arqueo Efectivo</label><br /><br />

                                </div>

                            </div>

                            <div class="form-group">
                                <div class="col-md-offset-5 col-md-10">
                                    <input type="submit" value="Guardar" class="btn btn-primary" />
                                    @Html.ActionLink("Cancelar", "Index", null, new { @class = "btn btn-white" })
                                </div>
                            </div>
                        </div>
                    }


                </div>
            </div>
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Auditoría</h5>
                </div>
                <div class="ibox-content">
                    <table class="table table table-striped  table-bordered">
                        <tr>
                            <th>Acción</th>
                            <th>Usuario</th>
                            <th>Fecha</th>
                        </tr>
                        <tr>
                            <td>Creado</td>
                            <td>
                                @Html.DisplayFor(model => model.tbUsuario.usu_Nombres)
                                @Html.DisplayFor(model => model.tbUsuario.usu_Apellidos)
                            </td>
                            <td>@Html.DisplayFor(model => model.mocja_FechaCrea)</td>
                        </tr>
                        <tr>
                            <td>Modificado</td>
                            <td>
                                @Html.DisplayFor(model => model.tbUsuario1.usu_Nombres)
                                @Html.DisplayFor(model => model.tbUsuario1.usu_Apellidos)
                            </td>
                            <td>@Html.DisplayFor(model => model.mocja_FechaModifica)</td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
