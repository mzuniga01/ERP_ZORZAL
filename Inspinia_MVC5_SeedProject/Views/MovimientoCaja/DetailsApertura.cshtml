@model ERP_GMEDINA.Models.tbMovimientoCaja

@{
    var solicitud = new ERP_GMEDINA.Models.UDV_Vent_MovimientoCaja_Apertura_Detalles();
}
<link href="~/Content/css/Auditoria.css" rel="stylesheet" />

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Aperturar Caja</h2>

    </div>
    <div class="col-sm-8">
        <div class="title-action">
            <a href="@Url.Action("IndexApertura", "MovimientoCaja")">Regresar</a>
        </div>
    </div>
</div>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Detalles Apertura</h5>
                </div>
                <div class="ibox-content">


	    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.mocja_Id)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.mocja_Id)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.mocja_FechaApertura)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.mocja_FechaApertura)
        </dd>
        <dt>
            Abreviatura Moneda
        </dt>
        <dd>
            @Html.DisplayFor(model => solicitud.mnda_Abreviatura)
        </dd>
        <dt>
           Nombre Moneda
        </dt>
        <dd>
            @Html.DisplayFor(model => solicitud.mnda_Nombre)
        </dd>
        <dt>
          Cantidad Solicitada
        </dt>
         <dd>
          @Html.DisplayFor(model => solicitud.soled_CantidadSolicitada)
        </dd>
         <dt>
           Cantidad Solicitada
         </dt>
        <dd>
        @Html.DisplayFor(model => solicitud.soled_CantidadEntregada)
        </dd>
        <dt>
          Monto Entregado
        </dt>
        <dd>
         @Html.DisplayFor(model => solicitud.soled_MontoEntregado)
        </dd>
    </dl>
   </div>
</div>

            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Auditoría</h5>
                </div>
                <div class="ibox-content">
                    <table class="table table-striped table-bordered">
                        <tr>
                            <th>Acción</th>
                            <th>Usuario</th>
                            <th>Fecha</th>
                        </tr>
                        <tr>
                            <td>Creado</td>
                            <td>
                                @Html.DisplayFor(model => model.tbUsuario.usu_Nombres)
                                @Html.DisplayFor(model => model.tbUsuario.usu_Apellidos)
                            </td>
                            <td>@Html.DisplayFor(model => model.mocja_FechaCrea)</td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
    </div>
 </div>













