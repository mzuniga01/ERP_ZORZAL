@model ERP_ZORZAL.Models.tbTipoSalida

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        @*<h2>Create</h2>
            <ol class="breadcrumb">
                <li>
                    @Html.ActionLink("List", "Index")
                </li>
                <li class="active">
                    <strong>Create</strong>
                </li>
            </ol>*@
    </div>
    <div class="col-sm-8">
        <div class="title-action">
            <a href="@Url.Action("Index","Box")">Regresar</a>
        </div>
    </div>
</div>




<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Editar Tipo de Salida</h5>
                </div>
                <div class="ibox-content">

                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()

                        <div class="form-horizontal">

                            @Html.ValidationSummary(true)
                            @Html.HiddenFor(model => model.tsal_Id)

                            <div class="form-group">
                                @Html.LabelFor(model => model.tsal_Descripcion, new { @class = "control-label col-md-2" })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.tsal_Descripcion)
                                    @Html.ValidationMessageFor(model => model.tsal_Descripcion)
                                </div>
                            </div>

                            <div class="form-group">
                                @Html.LabelFor(model => model.tsal_UsuarioCrea, new { @class = "control-label col-md-2" })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.tsal_UsuarioCrea)
                                    @Html.ValidationMessageFor(model => model.tsal_UsuarioCrea)
                                </div>
                            </div>

                            <div class="form-group">
                                @Html.LabelFor(model => model.tsal_FechaCrea, new { @class = "control-label col-md-2" })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.tsal_FechaCrea)
                                    @Html.ValidationMessageFor(model => model.tsal_FechaCrea)
                                </div>
                            </div>

                            <div class="form-group">
                                @Html.LabelFor(model => model.tsal_UsuarioModifica, new { @class = "control-label col-md-2" })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.tsal_UsuarioModifica)
                                    @Html.ValidationMessageFor(model => model.tsal_UsuarioModifica)
                                </div>
                            </div>

                            <div class="form-group">
                                @Html.LabelFor(model => model.tsal_FechaModifica, new { @class = "control-label col-md-2" })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.tsal_FechaModifica)
                                    @Html.ValidationMessageFor(model => model.tsal_FechaModifica)
                                </div>
                            </div>

                            <div class="form-group">
                                <div class="col-md-offset-2 col-md-10">
                                    <input type="submit" value="Guardar" class="btn btn-primary" />
                                    @Html.ActionLink("Cancelar", "Index", null, new { @class = "btn btn-white" })
                                </div>
                            </div>
                        </div>
                    }


                </div>
            </div>
        </div>
    </div>
</div>



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
