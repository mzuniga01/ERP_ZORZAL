@model ERP_GMEDINA.Models.tbEstadoFactura

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Estado Factura</h2>
 
    </div>
    <div class="col-sm-8">
        <div class="title-action">
            <a href="@Url.Action("Index","EstadoFactura")">Regresar</a>
        </div>
    </div>
</div>




<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Detalles Estado Factura</h5>
                </div>
                <div class="ibox-content">


				    <dl class="dl-horizontal">

                        <dt>
                            @Html.DisplayNameFor(model => model.esfac_Id)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.esfac_Id)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.esfac_Descripcion)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.esfac_Descripcion)
                        </dd>
                    </dl>


	    @Html.ActionLink("Editar", "Edit", new { id = Model.esfac_Id }, new { @class = "btn btn-primary"})


                </div>
            </div>

            <div class="ibox-content">
                <table class="table table-striped">
                    <tr>
                        <th>Acción</th>
                        <th>Usuario</th>
                        <th>Fecha</th>
                    </tr>
                    <tr>
                        <td>Creado</td>
                        <td>
                            @Html.DisplayFor(model => model.tbUsuario.usu_Nombres)
                            @Html.DisplayFor(model => model.tbUsuario.usu_Apellidos)
                    </td>
                         <td>@Html.DisplayFor(model => model.esfac_FechaCrea)</td>
                </tr>
                <tr>
                    <td>Modificado</td>
                    <td>
                    @Html.DisplayFor(model => model.tbUsuario1.usu_Nombres)
                    @Html.DisplayFor(model => model.tbUsuario1.usu_Apellidos)
                </td>
                <td>@Html.DisplayFor(model => model.esfac_FechaModifica)</td>
            </tr>
        </table>
        </div>

        </div>
    </div>
 </div>













