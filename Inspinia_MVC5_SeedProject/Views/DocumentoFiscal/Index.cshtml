@model IEnumerable<ERP_GMEDINA.Models.tbDocumentoFiscal>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Documento Fiscal</h2>
        @*<ol class="breadcrumb">
            <li class="active">
                <strong>tbDocumentoFiscal</strong>
            </li>
        </ol>*@
    </div>
</div>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Listado Documento Fiscal</h5>
					<div class="ibox-tools">
						@Html.ActionLink("Registrar Documento Fiscal", "Create", null, new { @class = "btn btn-primary btn-ms"})
					</div>
                </div>
                <div class="ibox-content">

<table class="table table table-striped  table-bordered"   id="DataTable">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.dfisc_Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dfisc_Descripcion)
            </th>

            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.dfisc_Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.dfisc_Descripcion)
                </td>

                <td>
                    @Html.ActionLink("Detalle", "Details", new { id = item.dfisc_Id }, new { @class = "btn btn-primary  btn-xs" })
                    @Html.ActionLink("Editar", "Edit", new { id = item.dfisc_Id }, new { @class = "btn btn-white  btn-xs" })
                </td>
            </tr>
        }
    </tbody>
</table>

                </div>
            </div>
        </div>
    </div>
 </div>


<script src="~/Scripts/jquery-3.3.1.js"></script>
<script src="~/Scripts/app/DataTable.js"></script>

