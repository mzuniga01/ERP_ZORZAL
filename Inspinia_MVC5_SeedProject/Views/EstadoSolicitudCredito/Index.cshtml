@model IEnumerable<ERP_GMEDINA.Models.tbEstadoSolicitudCredito>

@{
    ViewBag.Title = "ESC";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Estado Solicitud Crédito</h2>

    </div>
</div>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Listado de Estado Solicitud Crédito</h5>
					<div class="ibox-tools">
						@Html.ActionLink("Registrar Estado Solicitud Crédito", "Create", null, new { @class = "btn btn-primary"})
					</div>
                </div>
                <div class="ibox-content">

<table id="DataTable" class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.escre_Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.escre_Descripcion)
            </th>
        
            <th>Acciones</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.escre_Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.escre_Descripcion)
                </td>
         
                <td>
                    @Html.ActionLink("Detalles", "Details", new { id = item.escre_Id }, new { @class = "btn btn-primary btn-xs" })
                    @Html.ActionLink("Editar", "Edit", new { id = item.escre_Id }, new { @class = "btn btn-white btn-xs" })
              
                </td>
            </tr>
        }
    </tbody>
</table>

                </div>
            </div>
        </div>
    </div>
 </div>

<script src="~/Scripts/jquery-3.4.1.js"></script>
<script src="~/Scripts/app/DataTable.js"></script>

