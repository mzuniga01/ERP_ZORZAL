@model IEnumerable<ERP_GMEDINA.Models.tbRolesUsuario>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                @*<div class="ibox-content">*@
                    <table class="table table-striped table-responsive table-bordered table-hover" style="width:100%" id="DataTable">
                        <thead>
                            <tr style="font-weight: bold">
                                <th class="col-lg-4">
                                    @*@Html.DisplayNameFor(model => model.obj_Id)*@
                                    Código
                                </th>
                                <th class="col-lg-4">
                                    @*@Html.DisplayNameFor(model => model.tbObjeto.obj_Pantalla)*@
                                    Rol
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {

                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.rol_Id)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.tbRol.rol_Descripcion)
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                @*</div>*@
            </div>
        </div>
    </div>
 </div>

