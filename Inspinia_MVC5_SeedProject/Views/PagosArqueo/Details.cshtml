@model ERP_GMEDINA.Models.tbPagosArqueo

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/Content/css/Auditoria.css" rel="stylesheet" />

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Pagos Arqueo</h2>

    </div>
    <div class="col-sm-8">
        <div class="title-action">
            @Html.ActionLink("Regresar", "Index")
        </div>
    </div>
</div>





<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Detalle Pagos Arqueo</h5>
                </div>
                <div class="ibox-content">


				    <dl class="dl-horizontal">

        <dt>
            @Html.DisplayNameFor(model => model.tbMovimientoCaja.mocja_Id)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tbMovimientoCaja.mocja_Id)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.tbTipoPago.tpa_Descripcion)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tbTipoPago.tpa_Descripcion)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.arqpg_PagosSistema)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.arqpg_PagosSistema)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.arqpg_PagosConteo)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.arqpg_PagosConteo)
        </dd>
    </dl>


	    @Html.ActionLink("Edit", "Edit", new { id = Model.arqpg_Id }, new { @class = "btn btn-primary"})

                </div>
            </div>

            <div class="ibox-content">
                <table class="table table-striped">
                    <tr>
                        <th>Acción</th>
                        <th>Usuario</th>
                        <th>Fecha</th>
                    </tr>
                    <tr>
                        <td>Creado</td>
                        <td>
                            @Html.DisplayFor(model => model.tbUsuario.usu_Nombres)
                            @Html.DisplayFor(model => model.tbUsuario.usu_Apellidos)
                        </td>
                        <td>@Html.DisplayFor(model => model.arqpg_FechaCrea)</td>
                    </tr>
                    <tr>
                        <td>Modificado</td>
                        <td>
                            @Html.DisplayFor(model => model.tbUsuario1.usu_Nombres)
                            @Html.DisplayFor(model => model.tbUsuario1.usu_Apellidos)
                        </td>
                        <td>@Html.DisplayFor(model => model.arqpg_FechaModifica)</td>
                    </tr>
                </table>
            </div>
        </div>
    </div>
 </div>













