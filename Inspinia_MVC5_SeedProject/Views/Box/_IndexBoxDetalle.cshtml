@model IEnumerable<ERP_GMEDINA.Models.tbBoxDetalle>
@using ERP_GMEDINA.Models
@ViewData["clave"]
@{
    //tbSalidaDetalle SalidaDetalle = new tbSalidaDetalle();
}
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-content">
                    <input type="text" id="search" placeholder="Buscar Productos" class="form-control" />
                    <table id="Table_BuscarProducto" class="table table table-striped  table-bordered">
                        <thead>
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.prod_Codigo)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.tbProducto.prod_Descripcion)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.tbProducto.prod_Marca)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.tbProducto.prod_Modelo)
                                </th>
                            </tr>
                        </thead>

                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr tr data-id="@item.prod_Codigo" , tr data-content="@item.tbProducto.prod_Descripcion" , tr data-container="@item.tbProducto.tbProductoSubcategoria.pscat_Descripcion" , tr data-keyboard="@item.tbProducto.tbUnidadMedida.uni_Descripcion" , tr data-pcat="@item.tbProducto.tbProductoSubcategoria.tbProductoCategoria.pcat_Nombre">
                                    <td>
                                        @Html.DisplayFor(modelItem => item.tbProducto.prod_Descripcion)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.tbProducto.prod_Marca)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.tbProducto.prod_Modelo)
                                    </td>
                                    <th>
                                        @Html.DisplayFor(modelItem => item.boxd_Cantidad)
                                    </th>
                                </tr>
                            }
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>