@model IEnumerable<ERP_GMEDINA.Models.tbBodegaDetalle>
@using ERP_GMEDINA.Models

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-content">
                    <table id="Table_BuscarProducto" class="table table-striped table-responsive table-bordered table-hover">
                        <thead>
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.tbProducto.prod_CodigoBarras)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.tbProducto.prod_Codigo)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.tbProducto.prod_Marca)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.tbProducto.prod_Modelo)
                                </th>
                                <th hidden="hidden">
                                    @Html.DisplayNameFor(model => model.tbProducto.tbUnidadMedida.uni_Descripcion)
                                </th>
                                <th>
                                    Acciones
                                </th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}