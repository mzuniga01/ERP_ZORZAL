@model IEnumerable<ERP_GMEDINA.Models.tbActividadEconomica>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Actividad Económica</h2>
        <ol class="breadcrumb">
            <li class="active">
                <strong></strong>
            </li>
        </ol>
    </div>
</div>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Listado Actividad Económica</h5>
					<div class="ibox-tools">
						@Html.ActionLink("Registrar Actividad Económica", "Create", null, new { @class = "btn btn-primary" })
					</div>
                </div>
                <div class="ibox-content">

<table id="DataTable" class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.acte_Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.acte_Descripcion)
            </th>
            @*<th>
                @Html.DisplayNameFor(model => model.acte_UsuarioCrea)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.acte_FechaCrea)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.acte_UsuarioModifica)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.acte_FechaModifica)
            </th>*@
            <th>Acciones</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.acte_Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.acte_Descripcion)
                </td>
                @*<td>
                    @Html.DisplayFor(modelItem => item.acte_UsuarioCrea)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.acte_FechaCrea)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.acte_UsuarioModifica)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.acte_FechaModifica)
                </td>*@
                <td>
                    @Html.ActionLink("Detalles", "Details", new { id = item.acte_Id }, new { @class = "btn btn-primary btn-xs" })
                    @Html.ActionLink("Editar", "Edit", new { id = item.acte_Id }, new { @class = "btn btn-white btn-xs" })
                    
                </td>
            </tr>
        }
    </tbody>
</table>

                </div>
            </div>
        </div>
    </div>
 </div>

<script src="~/Scripts/jquery-3.4.1.js"></script>

<script src="~/Scripts/app/DataTable.js"></script>

