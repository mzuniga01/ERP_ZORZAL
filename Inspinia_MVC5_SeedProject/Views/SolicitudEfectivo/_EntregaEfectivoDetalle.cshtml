@model IEnumerable<ERP_GMEDINA.Models.tbSolicitudEfectivoDetalle>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-content">
                    <input type="hidden" name="solef_Id" id="solef_Id" value="@ViewBag.Entrega">
                    <table  id="EntregaEfectivo" class="table table-striped table-bordered table-hover dataTables-example">
                        <thead>
                            <tr>
                                <th class="hidden">
                                    @Html.DisplayNameFor(model => model.tbSolicitudEfectivo.solef_Id)
                                </th>
                                <th class="hidden">
                                    @Html.DisplayNameFor(model => model.soled_Id)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.tbDenominacion.deno_Descripcion)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.tbDenominacion.deno_valor)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.soled_CantidadSolicitada)
                                </th>
                                <th>
                                    Cantidad Entregada
                                </th>
                                <th>
                                    Monto Entregado
                                </th>
                                <th hidden>Id Denominacion</th>
                                <th hidden>Usuario Crea</th>
                                <th hidden>Fecha Crea</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td class="hidden">
                                        @Html.DisplayFor(modelItem => item.tbSolicitudEfectivo.solef_Id)
                                    </td>
                                    <td class="hidden">
                                        @Html.DisplayFor(modelItem => item.soled_Id)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.tbDenominacion.deno_Descripcion)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.tbDenominacion.deno_valor)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.soled_CantidadSolicitada)
                                    </td>
                                    <td>
                                        <input type="number" min="0" class="form-control" size="2" id="CantidadE" name="CantidadE" >

                                    </td>
                                    <td id="Prueba"></td>
                                    <td hidden>@Html.DisplayFor(modelItem => item.deno_Id)</td>
                                    <td hidden>@Html.DisplayFor(modelItem => item.soled_UsuarioCrea)</td>
                                    <td hidden>@Html.DisplayFor(modelItem => item.soled_FechaCrea)</td>
                                </tr>

                            }
                        </tbody>

                            <tfoot>
                                <tr>
                                    <td hidden></td>
                                    <td hidden></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td>Total</td>
                                    <td><p id="Total" style="text-align:right"></p></td>
                                </tr>

                            </tfoot>
                        </table>

                </div>
            </div>
        </div>
    </div>
</div>


<script src="~/Scripts/jquery-3.4.1.js"></script>
<script src="~/Scripts/app/DataTableBusquedaGenerica.js"></script>
<script src="~/Scripts/app/EntregaEfectivo.js"></script>