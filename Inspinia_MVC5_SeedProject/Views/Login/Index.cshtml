@model Inspinia_MVC5_SeedProject.Models.tbTiposPago

@{
    ViewBag.Title = "Index";
    Layout = "";
}
<link href="~/Content/css/master.css" rel="stylesheet" />

<div class="login-box">
    <img src="~/Content/img/MicrosoftTeams-image.png" class="avatar" alt="Avatar Image">
    <h1></h1>
    @using (Html.BeginForm())
    {
        <!-- USERNAME INPUT -->
        <label for="username">Nombre Usuario</label>
        @Html.EditorFor(model => model.tpago_Descripcion)
        <!-- PASSWORD INPUT -->
        <label for="password">Contraseña</label>
        @Html.EditorFor(model => model.tpago_Descripcion, new { htmlAttributes = new { @class = "form-control", type = "password" , placeholder = "Password" } })
        <input type="submit" value="INGRESAR">
        <a href="#">Olvidaste Tu Contraseña?</a><br>
        <a href="#"></a>
    }
</div>

