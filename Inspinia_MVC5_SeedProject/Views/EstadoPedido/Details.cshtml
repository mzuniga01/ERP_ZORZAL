@model ERP_GMEDINA.Models.tbEstadoPedido

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_LayoutFacturacion.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Estado Pedido</h2>
        </div>
        <div class="col-sm-8">
            <div class="title-action">
                <a href="/EstadoSolicitudCredito/Index">Regresar</a>
            </div>
        </div>
    </div>





<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Detalles Estado Pedido</h5>
                </div>
                <div class="ibox-content">


				    <dl class="dl-horizontal">

        <dt>
            @Html.DisplayNameFor(model => model.esped_Id)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.esped_Id)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.esped_Descripcion)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.esped_Descripcion)
        </dd>
    </dl>


	    @Html.ActionLink("Editar", "Edit", new { id = Model.esped_Id }, new { @class = "btn btn-primary"})

                </div>
            </div>
           


            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Auditoría</h5>
                </div>
                <div class="ibox-content">
                    <table class="table table table-striped  table-bordered">
                        <tr>
                            <th>Acción</th>
                            <th>Usuario</th>
                            <th>Fecha</th>
                        </tr>
                        <tr>
                            <td>Creado</td>
                            <td>
                                @Html.DisplayFor(model => model.tbUsuario.usu_Nombres)
                                @Html.DisplayFor(model => model.tbUsuario.usu_Apellidos)
                            </td>
                            <td>@Html.DisplayFor(model => model.esped_FechaCrea)</td>
                        </tr>
                        <tr>
                            <td>Modificado</td>
                            <td>
                                @Html.DisplayFor(model => model.tbUsuario1.usu_Nombres)
                                @Html.DisplayFor(model => model.tbUsuario1.usu_Apellidos)
                            </td>
                            <td>@Html.DisplayFor(model => model.esped_FechaModifica)</td>
                        </tr>
                    </table>
                </div>
            </div>



        </div>
    </div>
 </div>













