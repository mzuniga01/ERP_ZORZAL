@model ERP_GMEDINA.Models.tbInventarioFisicoDetalle
@using ERP_GMEDINA.Models



<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Editar Inventario Fisico Detalle</h5>
                </div>
                <div class="ibox-content">

			
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">

        @Html.ValidationSummary(true)
        @Html.HiddenFor(model => model.invfd_Id)
        @Html.HiddenFor(model => model.invf_Id)
        @Html.HiddenFor(model => model.invfd_UsuarioCrea)
        @Html.HiddenFor(model => model.invfd_FechaCrea)
        @Html.HiddenFor(model => model.invfd_UsuarioModifica)
        @Html.HiddenFor(model => model.invfd_FechaModifica)

        <div class="form-group">
            @Html.LabelFor(model => model.prod_Codigo, "Codigo Producto", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.prod_Codigo, new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.prod_Codigo)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.invfd_CantidadSistema, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.invfd_CantidadSistema)
                    @Html.ValidationMessageFor(model => model.invfd_CantidadSistema)
                </div>
        </div>

        @Html.HiddenFor(model => model.uni_Id)

        <div class="form-group">
            @Html.LabelFor(model => model.invfd_Cantidad,"Cantidad Fisica", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.invfd_Cantidad)
                    @Html.ValidationMessageFor(model => model.invfd_Cantidad)
                </div>
        </div>

    </div>

                    </div>
            </div>
        </div>
    </div>
 </div>



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
