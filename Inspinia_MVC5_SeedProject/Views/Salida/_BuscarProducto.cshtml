@model IEnumerable<ERP_GMEDINA.Models.tbBodegaDetalle>
@using ERP_GMEDINA.Models


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-content">
                    <input type="text" id="search" placeholder="Buscar Producto" class="form-control" />
                    <table id="Table_BuscarProducto" class="table table-hover">
                        <thead>
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.prod_Codigo)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.tbProducto.prod_Descripcion)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.tbProducto.tbProductoSubcategoria.tbProductoCategoria.pcat_Nombre)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.tbProducto.tbProductoSubcategoria.pscat_Descripcion)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.tbProducto.tbUnidadMedida.uni_Descripcion)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.tbProducto.prod_CodigoBarras)
                                </th>
                                <th>
                                    Acciones
                                </th>
                            </tr>
                        </thead>

                        <tbody id="Body_BuscarProducto"></tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}