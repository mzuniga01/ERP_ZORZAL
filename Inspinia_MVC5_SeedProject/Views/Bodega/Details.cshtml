@model ERP_GMEDINA.Models.tbBodega
@using ERP_GMEDINA.Models
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/css/Auditoria.css" rel="stylesheet" />
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Bodegas</h2>
    </div>
    <div class="col-sm-8">
        <div class="title-action">
            @Html.ActionLink("Regresar", "Index")
        </div>
    </div>
</div>




<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-content">
                    <div class="form-horizontal">
                        <dl class="dl-horizontal">
                            <dt>
                                @Html.DisplayNameFor(model => model.bod_Id)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.bod_Id)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.bod_Nombre)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.bod_Nombre)
                            </dd>
                            <dt>
                                @Html.DisplayNameFor(model => model.tbMunicipio.dep_Codigo)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.tbMunicipio.tbDepartamento.dep_Nombre)
                            </dd>
                            <dt>
                                @Html.DisplayNameFor(model => model.tbMunicipio.mun_Nombre)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.tbMunicipio.mun_Nombre)
                            </dd>
                            <dt>
                                @Html.DisplayNameFor(model => model.bod_ResponsableBodega)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.bod_ResponsableBodega)
                            </dd>
                            <dt>
                                @Html.DisplayNameFor(model => model.bod_Direccion)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.bod_Direccion)
                            </dd>
                            <dt>
                                @Html.DisplayNameFor(model => model.bod_Correo)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.bod_Correo)
                            </dd>
                            <dt>
                                @Html.DisplayNameFor(model => model.bod_Telefono)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.bod_Telefono)
                            </dd>
                            <dt>
                                @Html.DisplayNameFor(model => model.bod_EsActiva)
                            </dt>

                            <dd>
                                @{if (Model.bod_EsActiva == 1)
                                    {
                                        <p>Activa</p>
                                    }
                                    else
                                    {
                                        <p>Inactiva</p>;
                                    }


                                }
                            </dd>
                            @Html.ActionLink("Editar", "Edit", new { id = Model.bod_Id }, new { @class = "btn btn-primary" })
                        </dl>

                        <div class="row">
                            <div class="col-lg-12">
                                <div class="form-group">
                                    <div class="col-md-12">
                                        @Html.Partial("_ListBodegaDetalles", Model.tbBodegaDetalle, new ViewDataDictionary { { "bod_Id", Model.bod_Id } })
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Auditoría</h5>
                </div>
                <div class="ibox-content">
                    <table class="table table-striped  table-bordered">
                        <tr>
                            <th>Acción</th>
                            <th>Usuario</th>
                            <th>Fecha</th>
                        </tr>
                        <tr>
                            <td>Creado</td>
                            <td>
                                @Html.DisplayFor(model => model.tbUsuario.usu_Nombres)
                                @Html.DisplayFor(model => model.tbUsuario.usu_Apellidos)
                            </td>
                            <td>@Html.DisplayFor(model => model.bod_FechaCrea)</td>
                        </tr>
                        <tr>
                            <td>Modificado</td>
                            <td>
                                @Html.DisplayFor(model => model.tbUsuario1.usu_Nombres)
                                @Html.DisplayFor(model => model.tbUsuario1.usu_Apellidos)
                            </td>
                            <td>@Html.DisplayFor(model => model.bod_FechaModifica)</td>
                        </tr>
                    </table>
                </div>
            </div>

        </div>
    </div>
</div>




