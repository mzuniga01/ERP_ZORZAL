@model ERP_GMEDINA.Models.tbEntrada
@using ERP_GMEDINA.Models

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Entradas</h2>
        @*<ol class="breadcrumb">
                <li>
                    @Html.ActionLink("List", "Index")
                </li>
                <li class="active">
                    <strong>Details</strong>
                </li>
            </ol>*@
    </div>
    <div class="col-sm-8">
        <div class="title-action">
            <a href="@Url.Action("Index","Entrada")">Regresar</a>
            @*@Html.ActionLink("Regresar", "Index", null, new { @class = "btn btn-primary"})*@
        </div>
    </div>

</div>




<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Detalles Entrada</h5>
                </div>
                <div class="ibox-content">

                    <dl class="dl-horizontal">

                        <dt>
                            Nombre Bodega
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.tbBodega1.bod_Nombre)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.ent_NumeroFormato)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.ent_NumeroFormato)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.ent_FechaElaboracion)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.ent_FechaElaboracion)
                        </dd>
                        <dt>
                            Nombre Proveedor
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.tbProveedor.prov_Nombre)
                        </dd>

                        <dt>
                            @*@Html.DisplayNameFor(model => model.tbEstadoMovimiento.estm_Descripcion)*@
                            Estado Actual
                        </dt>

                        <dd>
                            @*@Html.DisplayFor(model => model.tbEstadoMovimiento.estm_Descripcion)*@
                            @if (Model.estm_Id == Helpers.EntradaEmitida)
                            {
                                <p>Emitida</p>
                            }
                            else if (Model.estm_Id == Helpers.EntradaInactivada)
                            {
                                <p>Inactiva</p>
                            }
                            else if (Model.estm_Id == Helpers.EntradaAplicada)
                            {
                                <p style="">APLICADA</p>
                            }
                            else if (Model.estm_Id == Helpers.EntradaEstadoAnulada)
                            {
                                <p style="">ANULADA</p>
                                <dt>
                                Razon Anulacion
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.entd_RazonAnulada)
                                </dd>
                            }
                        </dd>
                        <dt>
                            Tipo Entrada
                        </dt>
                        <dd>

                            @if (Model.tent_Id == 1)
                            {
                                @Html.DisplayFor(model => model.tbTipoEntrada.tent_Descripcion)

                            <dt>
                                @Html.DisplayNameFor(model => model.ent_FacturaCompra)
                            </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.ent_FacturaCompra)
                                    </dd>

                                    <dt>
                                        @Html.DisplayNameFor(model => model.ent_FechaCompra)
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.ent_FechaCompra)
                                    </dd>

                                    <dt>
                                        @Html.DisplayNameFor(model => model.fact_Id)
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.fact_Id)
                                    </dd>

                            }
                            else if (Model.tent_Id == 2)
                            {
                                @Html.DisplayFor(model => model.tbTipoEntrada.tent_Descripcion)
                                <dt>
                                    @Html.DisplayNameFor(model => model.ent_RazonDevolucion)
                                </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.tbTipoDevolucion.tdev_Descripcion)
                                    </dd>
                                    <dt>
                                        @Html.DisplayNameFor(model => model.fact_Id)
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.fact_Id)
                                    </dd>

                            }
                            else if (Model.tent_Id == 3)
                            {
                                @Html.DisplayFor(model => model.tbTipoEntrada.tent_Descripcion)
                                <dt>
                                    Bodega Destino
                                    @*@Html.DisplayNameFor(model => model.ent_BodegaDestino)*@
                                </dt>

                                    <dd>
                                        @Html.DisplayFor(model =>model.tbBodega.bod_Nombre)
                                    </dd>

                            }
                            @Html.ActionLink("Editar", "Edit", new { id = Model.ent_Id }, new { @class = "btn btn-primary" })

                        </dl>
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.Partial("_IndexDetalleEntrada", Model.tbEntradaDetalle)
                                </div>
                            </div>
                        </div>
                    </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
<div class="ibox float-e-margins">
    <div class="ibox-title">
        <h5>Auditoría</h5>
    </div>
    <div class="ibox-content">
        <table class="table table-striped table-bordered">
            <tr>
                <th>Acción</th>
                <th>Usuario</th>
                <th>Fecha</th>
            </tr>
            <tr>
                <td>Creado</td>
                <td>
                    @Html.DisplayFor(model => model.tbUsuario.usu_Nombres)
                    @Html.DisplayFor(model => model.tbUsuario.usu_Apellidos)
                </td>
                <td>@Html.DisplayFor(model => model.ent_FechaCrea)</td>
            </tr>
            <tr>
                <td>Modificado</td>
                <td>
                    @Html.DisplayFor(model => model.tbUsuario1.usu_Nombres)
                    @Html.DisplayFor(model => model.tbUsuario1.usu_Apellidos)
                </td>
                <td>@Html.DisplayFor(model => model.ent_FechaModifica)</td>
            </tr>
        </table>
    </div>
</div>
