@model IEnumerable<ERP_GMEDINA.Models.tbBox>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                   
                </div>
                <div class="ibox-content">

                    <table id="tblBusquedaGenericaBox" class="table table-striped table-responsive table-bordered table-hover" style="width:100%">
                        <thead>
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.box_Codigo)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.box_Descripcion)
                                </th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.box_Codigo)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.box_Descripcion)
                                    </td>
                                    <td>
                                        <button class="btn btn-primary btn-xs" value="@item.box_Codigo" id="seleccionarBox" data-dismiss="modal">Seleccionar</button>
                                    </td>
                                </tr>
                            }
                        </tbody>


                    </table>

                </div>
            </div>
        </div>
    </div>
 </div>



