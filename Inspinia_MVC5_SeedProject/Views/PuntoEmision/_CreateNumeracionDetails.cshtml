@model ERP_GMEDINA.Models.tbPuntoEmisionDetalle

<div class="wrapper wrapper-content">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
 
                <input name="pemi_Id" id="txtpemi_Id" type="hidden" value="@ViewBag.IdPuntoEmisionEdit">
                <div class="form-horizontal">
                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                    <div class="form-group">
                        <label class="control-label col-md-2">Número CAI</label>
                        <div class="col-md-10">
                            @Html.EditorFor(model => model.tbPuntoEmision.pemi_NumeroCAI, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-md-2">Documento Fiscal<span style="color:red"> *</span></label>
                        <div class="col-md-10">
                            @{ string Vie = "dfisc_IdList_";}
                            @Html.DropDownList("dfisc_Id", (IEnumerable<SelectListItem>)Context.Items[Vie], "Seleccione", new { @class = "form-control" })
                            <div id="validacionDocumentoFiscalCreate">
                                <p style="color:red">@Html.ValidationMessageFor(model => model.dfisc_Id)</p>
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="control-label col-md-2">Rango Inicial<span style="color:red"> *</span></label>
                        <div class="col-md-10">
                            @Html.EditorFor(model => model.pemid_RangoInicio, new { htmlAttributes = new { @onkeypress = "return validar(event)", @id = "txtRangoInicial" } })
                            <div id="validacionRangoInicioCreate">
                                <p style="color:red">@Html.ValidationMessageFor(model => model.pemid_RangoInicio)</p>
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="control-label col-md-2">Rango Final<span style="color:red"> *</span></label>
                        <div class="col-md-10">
                            @Html.EditorFor(model => model.pemid_RangoFinal, new { htmlAttributes = new { @onkeypress = "return validar(event)", @id = "txtRangoFinal" } })
                            <div id="validacionRangoFinalCreate">
                                <p style="color:red">@Html.ValidationMessageFor(model => model.pemid_RangoFinal)</p>
                            </div>
                        </div>

                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.pemid_NumeroActual, new { @class = "control-label col-md-2" })
                        <div class="col-md-10">
                            @Html.EditorFor(model => model.pemid_NumeroActual, new { htmlAttributes = new {@id="txtNumeroActual", @class = "form-control", @readonly = "readonly" } })
                            <div id="validacionNumeroActualCreate">
                                <p style="color:red">@Html.ValidationMessageFor(model => model.pemid_NumeroActual)</p>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-md-2">Fecha Límite Emisión<span style="color:red"> *</span></label>
                        <div class="col-md-10">
                            @Html.EditorFor(model => model.pemid_FechaLimite, "{0:MM/dd/yyyy}", new { htmlAttributes = new { @autocomplete = "off", @id = "txtFechalimite"} })
                            <div id="validacionFechaLimiteCreate">
                                <p style="color:red">@Html.ValidationMessageFor(model => model.pemid_FechaLimite)</p>
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <div>
                            <hr />
                            <div class="col-md-12 text-right">
                                <input type="submit" id="btnCreateNumeracionDetalle" value="Guardar" class="btn btn-primary" />
                                <button type="button" class="btn btn-white" data-dismiss="modal">Cerrar</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
