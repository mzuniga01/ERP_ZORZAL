@model IEnumerable<ERP_GMEDINA.Models.tbSolicitudEfectivoDetalle>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-content">
                    <h4 style="color:dodgerblue">Denominaciones Solicitadas</h4>
                    <table class="table table-striped" id="mitabla">
                        <thead>
                            <tr>
                                <th class="hidden">
                                    @Html.DisplayNameFor(model => model.tbSolicitudEfectivo.solef_Id)
                                </th>
                                <th class="hidden">
                                    @Html.DisplayNameFor(model => model.soled_Id)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.tbDenominacion.deno_Descripcion)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.soled_CantidadSolicitada)
                                </th>
                                <th>
                                    Monto Solicitaddo
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.soled_CantidadEntregada)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.soled_MontoEntregado)
                                </th>


                                <th></th>

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {

                                <tr>
                                    <td class="hidden">
                                        @Html.DisplayFor(modelItem => item.tbSolicitudEfectivo.solef_Id)
                                    </td>
                                    <td class="hidden">
                                        @Html.DisplayFor(modelItem => item.soled_Id)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.tbDenominacion.deno_Descripcion)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.soled_CantidadSolicitada)
                                    </td>
                                    @{
                                        decimal montoSolicitud = 0;
                                        montoSolicitud = Convert.ToDecimal(item.tbDenominacion.deno_valor) * Convert.ToDecimal(item.soled_CantidadSolicitada);
                                    }

                                    <td>
                                        @Html.DisplayFor(modelItem => montoSolicitud)
                                    </td>

                                    <td>
                                        @Html.DisplayFor(modelItem => item.soled_CantidadEntregada)
                                    </td>

                                    @{
                                        decimal MontoEntregado = 0;
                                        MontoEntregado = Convert.ToDecimal(item.tbDenominacion.deno_valor) * Convert.ToDecimal(item.soled_CantidadEntregada);
                                    }
                                    <td>
                                        @Html.DisplayFor(modelItem => MontoEntregado)
                                    </td>
                                    <td>
                                        <button  type="button" class="btn btn-primary btn-xs" data-toggle="modal" data-target="#Editar_@item.soled_Id">
                                            Editar
                                        </button>
                                    </td>
                                </tr>

                                <div class="modal fade" id="Editar_@item.soled_Id" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                                    <div class="modal-dialog" role="document">
                                        <div class="modal-content">
                                            <div class="modal-header">
                                                <h5 class="modal-title" id="exampleModalLabel">Editar Solicitud Efectivo Detalle</h5>
                                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                    <span aria-hidden="true">&times;</span>
                                                </button>
                                            </div>
                                            <div id="EditarSolicitudEfectivoDetalle" class="modal-body">
                                                @Html.Partial("_EditarSolicitudEfectivoDetalle", new ERP_GMEDINA.Models.tbSolicitudEfectivoDetalle() { solef_Id = item.solef_Id, soled_Id = item.soled_Id, deno_Id = item.deno_Id, soled_CantidadSolicitada = item.soled_CantidadSolicitada, soled_CantidadEntregada = item.soled_CantidadEntregada, soled_MontoEntregado = item.soled_MontoEntregado, soled_UsuarioCrea = item.soled_UsuarioCrea, soled_FechaCrea = item.soled_FechaCrea, soled_UsuarioModifica = item.soled_UsuarioModifica, soled_FechaModifica = item.soled_FechaModifica })
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                        }
                        </tbody>
                    </table>

                </div>
            </div>
        </div>
    </div>
</div>


<script src="~/Scripts/jquery-3.3.1.js"></script>
<script src="~/Scripts/app/DataTableBusquedaGenerica.js"></script>
