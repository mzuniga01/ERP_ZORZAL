@model IEnumerable<ERP_GMEDINA.Models.UDV_Vent_MovimientoCaja_Select>


<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css" integrity="sha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU" crossorigin="anonymous">
@{
    var sucursal = new ERP_GMEDINA.Models.tbSucursal();
}


<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Aperturar Caja</h2>
    </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Listado Caja</h5>
                    <div class="ibox-tools">
                        @Html.ActionLink("Registrar Apertura Caja", "CreateApertura", null, new { @class = "btn btn-primary" })
                    </div>
                </div>
                <div class="ibox-content">
                    <div class="form-horizontal">
                    <div class="form-group">
                        <label class="control-label col-md-1">Sucursal</label>
                        <div class="col-md-4">
                            @Html.EditorFor(model => sucursal.suc_Descripcion, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly", @Value = ViewBag.suc_Descripcion } })
                        </div>
                    </div>
                </div>
                    <table id="DataTable" class="table table table-striped  table-bordered" >
                        <thead>
                            <tr>
                                <th>
                                    Número
                                </th>
                                <th>
                                    Descripción
                                </th>
                                <th class="hidden">
                                 Id Solicitud
                                </th>
                                <th>Acciones</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.cja_Id)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.cja_Descripcion)
                                    </td>
                                    <td class="hidden">
                                        @Html.DisplayFor(modelItem => item.solef_Id)
                                    </td>
                                    <td>
                                        @Html.ActionLink("Imprimir", "ExportReportApertura", new { id = item.solef_Id }, new { @class = "btn btn-primary btn-xs", target = "_blank" })
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>

                </div>
            </div>
        </div>
    </div>
 </div>



<script src="~/Scripts/jquery-3.3.1.js"></script>
<script src="~/Scripts/app/Datepicker.js"></script>
<script src="~/Scripts/app/DataTable.js"></script>
