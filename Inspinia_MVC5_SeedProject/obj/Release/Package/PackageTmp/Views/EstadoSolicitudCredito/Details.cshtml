@model ERP_GMEDINA.Models.tbEstadoSolicitudCredito

@{
    ViewBag.Title = "ESC";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Estado Solicitud Crédito </h2>
     
    </div>
    <div class="col-sm-8">
        <div class="title-action">
            <a href="/EstadoSolicitudCredito/Index">Regresar</a>
        </div>
    </div>
</div>




<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Detalles Solicitud Créditos </h5>
                </div>
                <div class="ibox-content">
				    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.escre_Id)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.escre_Id)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.escre_Descripcion)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.escre_Descripcion)
                        </dd>
                    </dl>
                    @Html.ActionLink("Editar", "Edit", new { id = Model.escre_Id }, new { @class = "btn btn-primary" })
                </div>
            </div>
            <div class="ibox-content">
                <table class="table table-striped">
                    <tr>
                        <th>Acción</th>
                        <th>Usuario</th>
                        <th>Fecha</th>
                    </tr>
                    <tr>
                        <td>Creado</td>
                        <td>@Html.DisplayFor(model => model.tbUsuario.usu_Nombres)
                            @Html.DisplayFor(model => model.tbUsuario.usu_Apellidos)</td>
                        <td>@Html.DisplayFor(model => model.escre_FechaAgrego)</td>
                    </tr>
                    <tr>
                        <td>Modificado</td>
                        <td>@Html.DisplayFor(model => model.tbUsuario1.usu_Nombres)
                            @Html.DisplayFor(model => model.tbUsuario1.usu_Apellidos)</td>
                        <td>@Html.DisplayFor(model => model.escre_FechaModifica)</td>
                    </tr>
                </table>
            </div>
        </div>
    </div>
 </div>













